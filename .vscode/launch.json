    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "core.web.webapp",
                "OAUTHLIB_INSECURE_TRANSPORT": "1"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload",
                "--host",
                "0.0.0.0"
            ],
            "jinja": true,
            "justMyCode": false
        }
    ]
}
